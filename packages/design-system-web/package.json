{
  "name": "@geovistory/design-system-web",
  "version": "0.2.0",
  "description": "geovistory.org's design system: web components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/design-system-web/design-system-web.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geovistory/design-system.git"
  },
  "keywords": [
    "geovistory",
    "design-system",
    "web-components"
  ],
  "author": "geovistory.org",
  "bugs": {
    "url": "https://github.com/geovistory/design-system/issues"
  },
  "homepage": "",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "start": "npm run start:storybook",
    "dev": "npm run build-watch:stencil & start-storybook -p 6006 -s dist",
    "=========================== STENCIL ==========================": "",
    "generate": "npx stencil generate",
    "start:stencil": "npx stencil build --dev --watch --serve",
    "==================== BUILD ====================": "================================",
    "build": "npx stencil build",
    "build:docs": "npm run build:stencil && build-storybook -o ../../storybook  --quiet",
    "build:stencil": "npx stencil build --docs",
    "build-watch:stencil": "npx stencil build --watch",
    "build:storybook": "build-storybook",
    "==================== TEST =====================": "================================",
    "test": "npm run test:stencil",
    "test:stencil": "npx stencil test --spec --e2e",
    "test-watch:stencil": "npx stencil test --spec --e2e --watchAll",
    "========================== STORYBOOK =========================": "",
    "start:storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "@stencil/core": "^2.13.0",
    "@stencil/react-output-target": "^0.3.1",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@stencil/angular-output-target": "^0.4.0",
    "@storybook/addon-actions": "^6.5.5",
    "@storybook/addon-essentials": "^6.5.5",
    "@storybook/addon-links": "^6.5.5",
    "@storybook/html": "^6.5.5",
    "@types/jest": "^27.0.3",
    "babel-loader": "^8.2.5",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "puppeteer": "^10.0.0",
    "storybook-design-token": "^2.2.0"
  },
  "license": "MIT"
}
