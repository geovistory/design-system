  "homepage": "",
  "files": [
    "dist/",
    "loader/",
    "hydrate/"
  ],
  "scripts": {
    "postinstall": "patch-package",
    "start": "npm run start:storybook",
    "=========================== DEV ==========================": "",
    "dev": "concurrently --names \"ðŸ§©_STENCIL__,ðŸ“•_STORYBOOK\" -c \"blue,magenta\" \"npm:dev:watch\" \"npm:dev:docs\"",
    "dev:build": "npx stencil build --config stencil.dev.config.ts",
    "dev:watch": "stencil build --config stencil.dev.config.ts --watch",
    "dev:docs": "start-storybook -p 6006 --quiet",
    "=========================== STENCIL ==========================": "",
    "generate": "npx stencil generate",
    "start:stencil": "npx stencil build --dev --watch --serve",
    "==================== BUILD ====================": "================================",
    "build": "npx stencil build && node .build/stencil.post-build.js && rollup -c rollup.config.js",
    "build:docs": "npm run build:stencil && build-storybook -o ../../storybook  --quiet",
    "build:stencil": "npx stencil build --docs --config stencil.dev.config.ts && node .build/stencil.post-build.js",
    "build-watch:stencil": "npx stencil build --watch",
    "build:storybook": "build-storybook",
    "==================== TEST =====================": "================================",
    "test": "npm run test:stencil",
    "test:stencil": "npx stencil test --spec --e2e",
    "test-watch:stencil": "npx stencil test --spec --e2e --watchAll",
    "========================== STORYBOOK =========================": "",
    "start:storybook": "start-storybook -p 6006",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
