{
  "name": "@geovistory/design-system-web",
  "version": "1.0.3",
  "description": "geovistory.org's design system: web components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/design-system-web/design-system-web.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geovistory/design-system.git"
  },
  "keywords": [
    "geovistory",
    "design-system",
    "web-components"
  ],
  "author": "geovistory.org",
  "bugs": {
    "url": "https://github.com/geovistory/design-system/issues"
  },
  "homepage": "",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "start": "npm run start:storybook",
    "=========================== DEV ==========================": "",
    "dev": "npm run dev:build && concurrently --names \"ðŸ§©_STENCIL__,ðŸ“•_STORYBOOK\" -c \"blue,magenta\" \"npm:dev:watch\" \"npm:dev:docs\"",
    "dev:build": "npx stencil build --config stencil.dev.config.ts",
    "dev:watch": "stencil build --config stencil.dev.config.ts --watch",
    "dev:docs": "start-storybook -p 6006 --quiet",
    "=========================== STENCIL ==========================": "",
    "generate": "npx stencil generate",
    "start:stencil": "npx stencil build --dev --watch --serve",
    "==================== BUILD ====================": "================================",
    "build": "npx stencil build && node .build/stencil.post-build.js",
    "build:docs": "npm run build:stencil && build-storybook -o ../../storybook  --quiet",
    "build:stencil": "npx stencil build --docs --config stencil.dev.config.ts && node .build/stencil.post-build.js",
    "build-watch:stencil": "npx stencil build --watch",
    "build:storybook": "build-storybook",
    "==================== TEST =====================": "================================",
    "test": "npm run test:stencil",
    "test:stencil": "npx stencil test --spec --e2e",
    "test-watch:stencil": "npx stencil test --spec --e2e --watchAll",
    "========================== STORYBOOK =========================": "",
    "start:storybook": "start-storybook -p 6006",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@stencil/core": "2.13.0",
    "@stencil/react-output-target": "^0.3.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-icons": "^4.3.1",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@ionic/core": "6.0.14",
    "@stencil/angular-output-target": "^0.4.0",
    "@stencil/sass": "^1.5.2",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack4": "^6.5.9",
    "@storybook/manager-webpack4": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "^27.0.3",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.2.2",
    "ionicons": "^6.0.2",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "storybook-design-token": "^2.2.0"
  },
  "license": "MIT"
}
